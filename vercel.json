{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "rewrites": [
    {
      "source": "/robots.txt",
      "destination": "https://coiffeurs-lp-mvp.vercel.app/robots.txt"
    },
    {
      "source": "/sitemap.xml",
      "destination": "https://coiffeurs-lp-mvp.vercel.app/sitemap.xml"
    },
    {
      "source": "/favicon.ico",
      "destination": "https://coiffeurs-lp-mvp.vercel.app/favicon.ico"
    },
    {
      "source": "/og-image.png",
      "destination": "https://coiffeurs-lp-mvp.vercel.app/og-image.png"
    },
    { "source": "/assets/:path*", "destination": "https://coiffeurs-lp-mvp.vercel.app/assets/:path*" },
    { "source": "/", "destination": "https://coiffeurs-lp-mvp.vercel.app" },
    { "source": "/login", "destination": "https://coiffeurs-web.vercel.app/login" },
    { "source": "/:match*", "destination": "https://coiffeurs-web.vercel.app/:match*" }
  ]
}